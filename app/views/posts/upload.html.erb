<h3>Upload CSV File</h3>

<%= form_tag import_posts_path, multipart: true do %>
    <table>
        <tr>
            <td>
                <%= file_field_tag :file %>
            </td>
        </tr>
        <tr>
            <td>
                <%= submit_tag "Upload" %>
            </td>
        </tr>
    </table>
<% end %>